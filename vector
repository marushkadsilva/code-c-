#include <iostream>
#include<vector>
using namespace std;
int main() {
    
    vector<int>vec;
    vec.push_back(1);
    vec.push_back(2);
    vec.push_back(3);
    vec.push_back(4);
    vec.push_back(5);
    vec.push_back(6);
    vec.emplace_back(7);//adds element just like push
    
    vec.pop_back();//deletes last element
    
    for(int val:vec){
        cout<<val<<" ";
    }
cout<<endl;

cout<<vec[2];
cout<<vec.front()<<endl;
cout<<vec.back()<<endl;
    return 0;
}
